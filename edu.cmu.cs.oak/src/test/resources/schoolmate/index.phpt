	{
	 alert('You can only edit one user at a time.');
	}
	else
	{
	 alert('You must select a user to edit.');
	}
   }
  }


  // Function to keep track of how many checkboxes are checked //
  function updateboxes(row)
  {
   row = row + 2;
   if(document.users.elements[row].checked)
